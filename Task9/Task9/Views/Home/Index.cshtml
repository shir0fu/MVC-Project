@{
    Layout = null;
}
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@model IEnumerable<Task9.Models.Course>

<!doctype html>
<html>
<head>
    <title>TASK 9</title>
    <meta charset="utf-8"/>
</head>
<body>
    <h1>Courses</h1>
    @foreach (var item in Model)
    {
        <br><br/>
        <h2><a asp-action="Groups" asp-controller="Group" asp-route-id="@item.CourseId">@item.Name</a></h2>
    }
</body>
</html>